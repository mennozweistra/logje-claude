# Minimal Laravel Production Environment Configuration
# Copy these variables to your CapRover app's Environment Variables section

APP_NAME="Logje"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://logje.nl

# Database Configuration
# Replace with your actual MariaDB credentials in CapRover
DB_CONNECTION=mysql
DB_HOST=srv-captain--maria-db
DB_PORT=3306
DB_DATABASE=logje
DB_USERNAME=your_db_username
DB_PASSWORD=your_db_password

# Session Configuration
SESSION_DRIVER=file
SESSION_LIFETIME=120

# Cache Configuration
CACHE_DRIVER=file